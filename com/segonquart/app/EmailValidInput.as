class com.segonquart.app.EmailValidInput {		public function EmailValidInput()		{		}				public function checkEmailValidation(s:String):Boolean		{			var x:Number;			var t:Number=0;			var validateMe:Boolean=new Boolean();			for(var i:Number=0;i<s.length;i++)			{				if(s.charAt(i)==' ')				{					validateMe=false;					break;				}				if(s.charAt(i)=='@')				{					x=i;					t++;				}				else if(i>x&&s.charAt(i)=='.'&&s.charAt(s.length-1)!='.')				{					t++					validateMe=true					break;				}				else				{					if(i==s.length-1)					{						validateMe=false;						break;					}				}			}			return validateMe;		}	}}